.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
@font-face {
  font-family: 'iconfont';  /* project id 1335878 */
  src: url('//at.alicdn.com/t/font_1335878_ghfa52z3rzw.eot');
  src: url('//at.alicdn.com/t/font_1335878_ghfa52z3rzw.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1335878_ghfa52z3rzw.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1335878_ghfa52z3rzw.woff') format('woff'),
  url('//at.alicdn.com/t/font_1335878_ghfa52z3rzw.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1335878_ghfa52z3rzw.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

