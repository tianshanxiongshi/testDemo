/* pages/my-car/my-car.wxss */
.page {
  padding-bottom: 150rpx;
}
.line {
  width: 100%;
  height: 16rpx;
	background-color: #f4f5fa;
}
.handle {
  height: 76rpx;
  border-top: 1px solid #dadada;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ischooseImg {
  width: 24rpx;
  height: 24rpx;
}
.cancelImg {
  width: 26rpx;
	height: 29rpx;
}
.ischooseImg, .cancelImg {
  vertical-align: middle;
  margin-top: -6rpx;
  margin-right: 10rpx;
}
.handle view {
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 1rpx;
	color: #666666;
}
.carInfo image {
  width: 100rpx;
  height: 100rpx;
}
.carInfo {
  display: flex;
  align-items: center;
  padding: 20rpx 30rpx;
}
.carInfo .name {
  flex: 1;
  padding-left: 20rpx;
  font-size: 24rpx;
	font-weight: normal;
	font-stretch: normal;
	line-height: 36rpx;
	letter-spacing: 1rpx;
	color: #333333;
}
.addCar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 98rpx;
  width: 100%;
  line-height: 98rpx;
  text-align: center;
	background-color: #fa5a4c;
  font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1rpx;
	color: #ffffff;
}
.emptyTip {
  font-size:36rpx;
  text-align:center;
  color:#666;
  margin-top:300rpx;
}