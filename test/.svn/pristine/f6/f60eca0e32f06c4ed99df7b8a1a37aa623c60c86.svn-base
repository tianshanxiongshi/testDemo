  width: 750rpx;
  height: auto;
  background-color: #F3F5FA;
  border-bottom: 1rpx solid #DDDDDD;

}
.total .service_list .service_item{
  width: 690rpx;
  height: 104rpx;
  line-height: 104rpx;
  margin: 0 auto;
  overflow: hidden;
}

 .total .service_list .service_item text{
   
  font-size: 24rpx;
  color: #333333;

}
.total .service_list .service_item .te_t1{
    display: inline-block;
  float: right;
  color: #FA5A4C;

}
.integral{
  width: 690rpx;
  height: auto;
  margin: 0 auto;
}
.integral .v_1{
  float: left;
  line-height: 110rpx;
  font-size: 24rpx;
  color: #333333;
}
.integral .v_1 text{
 line-height: 110rpx;
  font-size: 24rpx;
  color: #333333;
}
.integral .v_2{
  float: left;
  width: 460rpx;
  height: auto;
  margin-left: 30rpx;
}

