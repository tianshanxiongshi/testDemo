  float: left;
  width: 74rpx;
  line-height: 26rpx;
  height: 26rpx;
  border: 1rpx solid #999999;
  border-radius: 4rpx;
  font-size: 16rpx;
  color: #FA5A4C;
  text-align: center;
  margin: 0 8rpx;
}
.integral .v_2 .vi_2 text{
  float: left;
font-size: 16rpx;
color: #333333;
height: 26rpx;
line-height: 26rpx;
}
