}
.integral  .v_3 text{
  width: 34rpx;
  height: 34rpx;
}
.pay{
  width: 750rpx;
  height: auto;
  margin-top: 20rpx;
  overflow: hidden;
}
.pay .confirm{
  float: right;
  width: 490rpx;
  height: 100rpx;
  background-color: #FA5A4C;
  color: #ffffff;
  font-size:30rpx;
  line-height: 100rpx;
  text-align: center;
}
.pay .real_pay{
  float: left;
  width: 260rpx;
  height: 100rpx;
}
.pay .real_pay text{
  float: left;
  width: 100rpx;
  text-align: center;
  font-size: 24rpx;
}
.pay .real_pay .text1{
   width: 100%;
   height: 32rpx;
   margin-top: 30rpx;
}
.pay .real_pay .text2{
   width: 100%;
   height: 32rpx;
